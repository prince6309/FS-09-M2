import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int[] arr=new int[n];
        for(int i=0;i<n;i++){
            arr[i]=sc.nextInt();
            
        }
        Arrays.sort(arr);
        int s=0;
        for(int i=0;i<n;i++){
            s=arr[n-1];
        }
        System.out.println(s);
    }
}
