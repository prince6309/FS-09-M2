import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int b=sc.nextInt();
        int c=sc.nextInt();
        boolean ans=func(a,b,c);
        System.out.println(ans);
    }
    public static boolean func(int a,int b,int c){
        int x=a+b;
        if(x>c){
            return true;
        }
        else
            return false;
    }
}
